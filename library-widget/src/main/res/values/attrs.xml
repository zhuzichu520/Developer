<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AutoPlayRecyclerView">
        <attr name="timeInterval" format="integer" />
        <attr name="direction" format="enum">
            <enum name="left" value="1" />
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="NiceToolbar">
        <attr name="toolbarTitle" format="string" />
        <attr name="toolbarStartText" format="string" />
        <attr name="toolbarStartIcon" format="reference" />
        <attr name="toolbarEndText" format="string" />
        <attr name="toolbarEndIcon" format="reference" />
        <attr name="toolbarTitleTextAppearance" format="reference" />
    </declare-styleable>

</resources>